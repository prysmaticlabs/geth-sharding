{
  "Deadline": "10m",
  "Exclude": [
    "^proto/",
    "^contracts/"
  ],
  "DisableAll": true,
  "Enable": [
      "deadcode",
      "dupl",
      "errcheck",
      "gochecknoglobals",
      "gochecknoinits",
      "goconst",
      "goimports",
      "golint",
      "gosec",
      "gosimple",
      "gotype",
      "megacheck",
      "misspell",
      "staticcheck",
      "structcheck",
      "unparam",
      "unused",
      "vet",
      "varcheck"
    ]
}
